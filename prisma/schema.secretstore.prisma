generator client {
  provider = "prisma-client-js"
  output   = "./generated/secretstore"
}

datasource secretdb {
  provider = "postgresql"
  url      = env("SECRET_STORE_DATABASE_URL")
}

model Secret {
  id            String   @id @default(uuid())
  walletId      String
  domain        String
  encryptedBlob String
  guardians     Json?
  scope         String?
  createdAt     DateTime @default(now())
}
